{
  "name": "nestjs-template",
  "version": "0.0.0",
  "description": "Node.js framework NestJS project structure",
  "main": "dist/app",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "entity": "node bin/entity",
    "build": "rimraf dist && nest build",
    "prestart": "npm run lint && npm run build",
    "start": "node dist/app",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "rimraf dist && nest start --debug --watch",
    "test": "jest --detectOpenHandles -i",
    "test:e2e": "jest --config ./test/jest.e2e.js -i",
    "doc": "compodoc -p tsconfig.json -s -d docs",
    "doc:api": "npm run build && node dist/swagger"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.2",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/terminus": "^9.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "apollo-server-express": "^3.10.2",
    "aws-sdk": "^2.1225.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.6",
    "helmet": "^6.0.0",
    "mysql2": "^2.3.3",
    "nanoid": "^3.1.30",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/testing": "^9.1.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/helmet": "0.0.48",
    "@types/jest": "^29.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.23",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-sonarjs": "^0.15.0",
    "jest": "^29.1.1",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.2",
    "typeorm-model-generator": "^0.4.6",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "Node.js",
    "NestJS",
    "TypeScript"
  ],
  "author": "HoeuSophanorin",
  "license": "MIT"
}
